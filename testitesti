proc freq data=poh2 noprint;
where kunta=10;
tables paikka / out=lah;
data lah2;
set lah;
rename count=lahet;
run;
proc freq data=poh2 noprint;
where kunta=10 and vast=1;
tables paikka / out=vas;
data vas2;
set vas;
rename count=vast;
run;
data yht1;
merge lah2(in=i) vas2;
by paikka;
if i;
data yht2;
set yht1;
if vast=. then vast=0;
IF lahet>=5;
vastpros=round(vast/lahet*100,0.1);
satu=ranuni(1712);
proc sort data=yht2;
by satu;
run;
